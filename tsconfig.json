{
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "sourceMap": true,
    "outDir": "./dist",
    "target": "es5",
    "strict": true,
    "esModuleInterop": true,
  },
  "include": [
    "src/scripts/**/*",
    "src/test/**/*",
  ],
  "exclude": [
    ".src/electron/*"
  ],
}